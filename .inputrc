"\e[B": history-search-forward  # down
"\e[A": history-search-backward # up
"\e[1;5C": forward-word
"\e[1;5D": backward-word
# kvt:
"\e[H":beginning-of-line
"\e[F":end-of-line
# rxvt and konsole (i.e. the KDE-app...)
"\e[7~":beginning-of-line
"\e[8~":end-of-line
# for linux console
"\e[1~": beginning-of-line
"\e[4~": end-of-line

$include /etc/inputrc
